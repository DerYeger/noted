{
  "name": "noted",
  "version": "1.25.45",
  "description": "Markdown note-taking with syntax highlighting and real-time preview.",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate --fail-on-error",
    "prepare": "husky install",
    "lint": "yarn lint:prettier && yarn lint:es && yarn lint:style",
    "lint:prettier": "prettier --check \"./**/*.{js,json,html,scss,ts,vue,yml}\"",
    "lint:es": "eslint \"./**/*.{js,ts,vue}\"",
    "lint:style": "stylelint \"./**/*.{css,ts,vue}\" --ignore-path .gitignore",
    "test": "jest",
    "pretty": "prettier --write \"./**/*.{js,json,html,scss,ts,vue,yml}\"",
    "ci": "yarn pretty && yarn lint && yarn test && yarn generate",
    "storybook": "yarn nuxt storybook"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxtjs/composition-api": "0.31.0",
    "@nuxtjs/i18n": "7.2.0",
    "@nuxtjs/pwa": "3.3.5",
    "@nuxtjs/sitemap": "2.4.0",
    "@yeger/vue2-masonry-wall": "2.0.17",
    "core-js": "3.21.0",
    "dompurify": "2.3.5",
    "indent-textarea": "2.1.0",
    "jszip": "3.7.1",
    "marked": "4.0.12",
    "nuxt-edge": "latest",
    "prism-theme-vars": "0.2.2",
    "prismjs": "1.26.0",
    "splitpanes": "2.3.8",
    "uuid": "8.3.2",
    "vue": "2.6.14",
    "vue-material-design-icons": "5.0.0",
    "vuex": "3.6.2",
    "vuex-persistedstate": "4.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.17.0",
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@nuxt/bridge": "npm:@nuxt/bridge-edge",
    "@nuxt/types": "2.15.8",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/color-mode": "2.1.1",
    "@nuxtjs/eslint-config-typescript": "8.0.0",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/storybook": "4.2.0",
    "@nuxtjs/stylelint-module": "4.1.0",
    "@types/dompurify": "2.3.3",
    "@types/marked": "4.0.2",
    "@types/prismjs": "1.26.0",
    "@types/uuid": "8.3.4",
    "@vue/test-utils": "1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.5.1",
    "css-loader": "4.3.0",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-nuxt": "3.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.4.1",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.3",
    "nuxt-font-loader": "1.1.3",
    "postcss": "8.4.6",
    "postcss-html": "1.3.0",
    "prettier": "2.5.1",
    "stylelint": "14.5.0",
    "stylelint-config-html": "1.0.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "24.0.0",
    "ts-jest": "27.1.3",
    "ts-node": "10.5.0",
    "vue-eslint-parser": "8.2.0",
    "vue-jest": "3.0.7"
  }
}
