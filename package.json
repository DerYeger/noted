{
  "name": "noted",
  "version": "1.20.7",
  "description": "Real-time note-taking with syntax highlighting.",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate --fail-on-error",
    "lint:es": "eslint \"./**/*.{js,ts,vue}\"",
    "lint:style": "stylelint \"./**/*.{css,ts,vue}\" --ignore-path .gitignore",
    "prepare": "husky install",
    "lint": "yarn lint:es && yarn lint:style",
    "test": "jest",
    "pretty": "prettier --write \"./**/*.{js,json,html,scss,ts,vue,yml}\"",
    "ci": "yarn pretty && yarn lint && yarn test && yarn generate",
    "storybook": "yarn nuxt storybook"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxtjs/composition-api": "0.29.2",
    "@nuxtjs/i18n": "7.0.3",
    "@nuxtjs/pwa": "3.3.5",
    "@nuxtjs/sitemap": "2.4.0",
    "@yeger/vue2-masonry-wall": "2.0.1",
    "core-js": "3.18.2",
    "dompurify": "2.3.3",
    "indent-textarea": "2.1.0",
    "jszip": "3.7.1",
    "marked": "3.0.7",
    "nuxt": "2.15.8",
    "prism-theme-vars": "0.2.2",
    "prismjs": "1.25.0",
    "splitpanes": "2.3.8",
    "uuid": "8.3.2",
    "vue": "2.6.14",
    "vue-material-design-icons": "4.13.0",
    "vuex": "3.6.2",
    "vuex-persistedstate": "4.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.15.8",
    "@commitlint/cli": "13.2.0",
    "@commitlint/config-conventional": "13.2.0",
    "@nuxt/types": "2.15.8",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/color-mode": "2.1.1",
    "@nuxtjs/eslint-config-typescript": "6.0.1",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/storybook": "4.1.1",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@types/dompurify": "2.3.1",
    "@types/marked": "3.0.1",
    "@types/prismjs": "1.16.6",
    "@types/uuid": "8.3.1",
    "@vue/test-utils": "1.2.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.2.5",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "7.19.1",
    "husky": "7.0.2",
    "jest": "27.2.5",
    "lint-staged": "11.2.0",
    "nuxt-font-loader": "1.1.3",
    "postcss": "8.3.9",
    "prettier": "2.4.1",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "22.0.0",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "vue-eslint-parser": "7.11.0",
    "vue-jest": "3.0.7"
  }
}
