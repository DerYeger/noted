{
  "name": "noted",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate --fail-on-error",
    "lint:js": "eslint --ext \".js,.vue,.ts\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "prepare": "husky install",
    "lint": "yarn lint:js && yarn lint:style",
    "test": "jest",
    "pretty": "prettier --write \"./**/*.{js,json,html,scss,ts,vue,yml}\"",
    "ci": "yarn pretty && yarn lint && yarn test && yarn generate"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxtjs/composition-api": "0.29.0",
    "@nuxtjs/i18n": "7.0.3",
    "@nuxtjs/pwa": "3.3.5",
    "@nuxtjs/sitemap": "2.4.0",
    "@yeger/vue2-masonry-wall": "1.1.5",
    "core-js": "3.15.1",
    "marked": "3.0.3",
    "nuxt": "2.15.8",
    "prismjs": "1.24.1",
    "splitpanes": "2.3.8",
    "uuid": "8.3.2",
    "vue": "2.6.14",
    "vuex": "3.6.2",
    "vuex-persistedstate": "4.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.14.7",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@nuxt/types": "2.15.8",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/eslint-config-typescript": "6.0.1",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@types/marked": "3.0.1",
    "@types/prismjs": "1.16.6",
    "@types/uuid": "8.3.1",
    "@vue/test-utils": "1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.0.5",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-vue": "7.17.0",
    "husky": "6.0.0",
    "jest": "27.0.5",
    "lint-staged": "11.1.2",
    "nuxt-font-loader": "1.1.3",
    "postcss": "8.3.5",
    "prettier": "2.3.2",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "22.0.0",
    "ts-jest": "27.0.3",
    "vue-jest": "3.0.4"
  }
}
